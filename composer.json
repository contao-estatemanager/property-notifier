{
  "name":"contao-estatemanager/property-notifier",
  "type":"contao-bundle",
  "description":"Property Notifier Extension for Contao EstateManager",
  "keywords":["contao","estatemanager"],
  "homepage":"https://www.contao-estatemanager.com/",
  "license":"proprietary",
  "authors":[
    {
      "name":"Oveleon",
      "homepage":"https://www.oveleon.de/",
      "role":"Developer"
    }
  ],
  "require":{
    "php":"^7.4 || ^8.0",
    "contao/core-bundle":"^4.13",
    "contao-estatemanager/core": "^1.1"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "contao/easy-coding-standard": "^3.4"
  },
  "conflict": {
    "contao/core": "*",
    "contao/core-bundle": "4.4.1",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload":{
    "psr-4": {
      "ContaoEstateManager\\PropertyNotifier\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra":{
    "contao-manager-plugin": "ContaoEstateManager\\PropertyNotifier\\ContaoManager\\Plugin"
  },
  "scripts": {
    "cs-fixer": [
      "vendor/bin/ecs check src/ --fix --ansi"
    ]
  }
}
